@model Wizzarts.Web.ViewModels.WizzartsMember.WizzartsTeamListViewModel

<div class="card mb-2 ">
    <div class="card-body p-2">
        <div class="d-flex flex-wrap justify-content-start">
            @foreach (var artist in this.Model.wizzartsTeam)
            {
                <div class="home-meta-card">
                    <div class="home-meta-card-headline lazy" style="background-image: url(@artist.AvatarUrl);background-repeat:no-repeat;background-position:left;background-size:90px;text-align:right;padding:5px;height:80px;">
                        @artist.Nickname
                    </div>
                    <ul>
                        <li><a asp-controller="Art" asp-action="ByUserId" asp-route-id="@artist.Id">Art</a></li>
                        <li><a href="/Metagame/Traditional-Standard/">BO3 Metagame</a></li>
                        <li><a href="/Events/Standard/">Tournament Decks</a></li>
                        <li><a href="/MTGA-Decks/Standard-BO1/">Latest MTGA Decks</a></li>
                        <li><a href="/Decks/Standard-BO1/">Player Posted Decks</a></li>
                    </ul>
                </div>
            }

        </div>
    </div>
</div>